#include<bits/stdc++.h>
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;

void solution(){
    string s; getline(cin,s);
    stringstream ss(s);
    string numbers;
    int cntr=0;

    while(ss>>numbers){
        cntr++;
    }

    cout<<cntr<<endl;

}

int main()
{
    optimize();
    int tc; cin>>tc;
    cin.ignore();
    while(tc--){
        solution();
    }

    return 0;
}
