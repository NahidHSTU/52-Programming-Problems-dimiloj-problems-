#include<bits/stdc++.h>
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;

void solution(){
    
   int n; cin>>n;
   int square_number= sqrt(n);

   if(square_number*square_number == n){

        cout<<"YES"<<endl;
   }
   else{
        cout<<"NO"<<endl;
   }

}

int main()
{
    optimize();
    int tc; cin>>tc;
    while(tc--){
        solution();
    }

    return 0;
}
