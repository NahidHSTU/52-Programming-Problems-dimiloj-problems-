#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        string num;
        cin >> num;
        
        int first_digit = num[0] - '0';   
        int last_digit = num[num.length() - 1] - '0';
        int sum_digits = first_digit + last_digit;

        cout << "Sum = " << sum_digits << endl;
    }

    return 0;
}
