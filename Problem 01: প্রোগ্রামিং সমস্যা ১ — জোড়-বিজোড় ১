#include<bits/stdc++.h>
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;

void solution(){
    long long n; cin>>n;

    if(n%2==0){
    	cout<<"even"<<endl;
    }
	else{
		cout<<"odd"<<endl;
	}

}

int main()
{
    optimize();
    int tc; cin>>tc;
    while(tc--){
        solution();
    }

    return 0;
}
